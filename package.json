{
  "name": "11ty-snowpack",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run snowpack:dev",
    "snowpack:dev": "snowpack dev",
    "snowpack:build": "snowpack build",
    "build": "npm-run-all build:svelte build:html",
    "build:html": "eleventy",
    "build:svelte": "rollup -c",
    "dev:svelte": "rollup -c -w",
    "watch": "npm-run-all --parallel dev:svelte watch:html",
    "watch:html": "eleventy --serve --port=8181",
    "start:rollup": "npm run watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "sirv-cli": "^2.0.1",
    "snowpack": "^3.8.8",
    "terser": "^5.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-svelte": "^3.7.0",
    "rollup": "^2.64.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.2"
  }
}
