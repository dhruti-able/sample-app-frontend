{
  "name": "11ty-snowpack",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run snowpack:dev",
    "snowpack:dev": "snowpack dev",
    "snowpack:build": "snowpack build",
    "build": "npm-run-all build:svelte build:html",
    "build:html": "eleventy",
    "build:svelte": "rollup -c",
    "dev:svelte": "rollup -c -w",
    "watch": "npm-run-all --parallel dev:svelte watch:html",
    "watch:html": "eleventy --serve --port=8181",
    "start:rollup": "npm run watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "axios": "^0.25.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "sirv-cli": "^2.0.1",
    "snowpack": "^3.8.8",
    "terser": "^5.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/preset-env": "^7.16.10",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-svelte": "^3.7.0",
    "@testing-library/jest-dom": "^5.16.1",
